		<parameter name="url" value="https://google.com"></parameter>
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="testngSessions.GoogleTest"></class>
		</classes>
	</test>
	<test name="OpenCart Test">
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?"></parameter>
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="testngSessions.OpenCartTest"></class>
		</classes>
	</test>
	<test name="Amazon Test">
		<parameter name="url" value="https://amazon.in"></parameter>
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="testngSessions.AmazonTest"></class>
		</classes>
	</test>
	
</suite>